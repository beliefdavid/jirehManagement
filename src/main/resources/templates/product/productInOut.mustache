{{>layouts/header}}

<div class="contener">

    <div class="form">
        <!-- 입출고 선택  -->
        <label for="productInOut" class="form-label">입/출고</label>
        <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example" id="productInOut">
            <option selected>입고/출고를 선택하세요.</option>
            <option value="1">입고</option>
            <option value="2">출고</option>
        </select>

        <!-- 부서명  -->
        <label for="department" class="form-label">부서명</label>
        <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example" id="department">
            <option selected>부서를 선택하세요.</option>
            <option value="1">시스템 개발부</option>
            <option value="2">영업부</option>
        </select>

        <!-- 요청자  -->
        <label for="requestor" class="form-label">요청자</label>
        <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example" id="requestor">
            <option selected>요청자를 선택하세요.</option>
            <option value="1">김선호</option>
            <option value="2">강구범</option>
            <option value="3">김민규</option>
            <option value="4">장석민</option>
        </select>


        <!-- 자재명  -->
        <label for="productName" class="form-label">자재명</label>
        <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example" id="productName">
            <option selected>자재를 선택하세요</option>
            <option value="1">JET-CM</option>
            <option value="2">JET-EM</option>
            <option value="3">JET-EMS</option>
            <option value="4">JET-EUI</option>
        </select>

        <!-- 제조사 -->
        <label for="productMaker" class="form-label">자재명</label>
        <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example" id="productMaker">
            <option selected>제조사를 선택하세요.</option>
            <option value="1">이레에너지테크</option>
            <option value="2">릴라이어블</option>
            <option value="3">지멘스</option>
            <option value="4">신한밸브</option>
        </select>

        <!-- 현장명  -->
        <label for="fieldName" class="form-label">현장명</label>
        <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example" id="fieldName">
            <option selected>현장명을 선택하세요</option>
            <option value="1">화성 봉담 A-1BL</option>
            <option value="2">송파 위례 A1-9BL</option>
            <option value="3">남양주 별내 A-13BL</option>
            <option value="4">남양주 별내 A-24BL</option>
        </select>

        <!-- 입/출고 수량 -->
        <div class="mb-3">
            <label for="productCheckNumber" class="form-label">입/출고 수량</label>
            <input type="email" class="form-control form-control-lg" id="productCheckNumber" aria-describedby="productCheckNumberW">
            <div id="productCheckNumberW" class="form-text">입출고 할려는 수량을 기입하세요.</div>
        </div>

        <!-- 특이사항 -->
        <div class="mb-3">
            <label for="productBigo" class="form-label">비고</label>
            <textarea class="form-control" placeholder="특이사항을 입력하세요." id="productBigo" style="height: 100px"></textarea>
        </div>
    </div>

</div>


{{>layouts/footer}}